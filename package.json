{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development node server.js",
    "start:dev": "cross-env NODE_ENV=test node server.js",
    "start:test": "cross-env NODE_ENV=test nodemon  server.js",
    "start:prod": "cross-env NODE_ENV=production node server.js",
    "dev": "cross-env NODE_ENV=test nodemon server.js",
    "seed": "cross-env NODE_ENV=development node app/seeder/index.js",
    "seed:test": "cross-env NODE_ENV=test node app/seeder/index.js",
    "seed:prod": "cross-env NODE_ENV=production node app/seeder/index.js",
    "migrate:create:test": "cross-env NODE_ENV=test app/config/migration.config create",
    "migrate:create": "cross-env NODE_ENV=development migrate-mongo create -f app/config/migration.config.js",
    "migrate:up": "cross-env NODE_ENV=development DEBUG=* migrate-mongo up -f app/config/migration.config.js",
    "migrate:down": "migrate-mongo down -f app/config/migration.config.js",
    "migrate:status": "migrate-mongo status -f app/config/migration.config.js"
  },
