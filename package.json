    "@aws-sdk/client-s3": "^3.857.0",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "express-form-data": "^3.0.1",
    "express-mongo-sanitize": "^2.2.0",
    "http-status": "^2.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.15.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemon": "^3.1.10",
    "pg": "^8.16.0",
    "redis": "^4.7.1",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.3",
    "typeorm": "^0.3.24",
    "validator": "^13.15.15",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "migrate-mongo": "^12.1.3"
  }
